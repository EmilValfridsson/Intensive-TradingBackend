generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("POSTGRES_PRISMA_URL")
}

model User {
  id       String  @id @default(cuid())
  name     String
  username String  @unique
  password String
  isAdmin  Boolean @default(false)
}

model Stock {
  name      String
  symbol    String  @unique
  date      String
  high      Int
  low       Int
  close     Int
  volume    Int
  isFavored Boolean @default(false)
}
